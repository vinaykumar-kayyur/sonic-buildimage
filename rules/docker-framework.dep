DPATH       := $($(DOCKER_FRAMEWORK)_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/docker-framework.mk rules/docker-framework.dep
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
