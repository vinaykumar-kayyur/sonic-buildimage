
SPATH       := $($(SONIC_PAC)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/pac.mk rules/pac.dep
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(SONIC_PAC)_CACHE_MODE  := GIT_CONTENT_SHA
$(SONIC_PAC)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(SONIC_PAC)_DEP_FILES   := $(DEP_FILES)
$(SONIC_PAC)_SMDEP_FILES := $(SMDEP_FILES)
$(SONIC_PAC)_SMDEP_PATHS := $(SPATH)

