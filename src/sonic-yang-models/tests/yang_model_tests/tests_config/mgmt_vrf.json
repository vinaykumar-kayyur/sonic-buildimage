            }
        }
    },
    "MGMT_VRF_TEST_WITH_IN_BAND_MGMT_ENABLED": {
        "sonic-mgmt_vrf:sonic-mgmt_vrf": {
            "sonic-mgmt_vrf:MGMT_VRF_CONFIG": {
                "sonic-mgmt_vrf:vrf_global": {
                    "mgmtVrfEnabled": true,
                    "in_band_mgmt_enabled": true
                }
            }
        }
    },
    "MGMT_VRF_TEST_WITH_DEFAULT_VALUE": {
        "sonic-mgmt_vrf:sonic-mgmt_vrf": {
            "sonic-mgmt_vrf:MGMT_VRF_CONFIG": {
